{
    "name": "@arturoalcaniz/users-service",
    "version": "1.3.1",
    "description": "Backend of TFG Project using nestjs",
    "author": "Arturo Alca√±iz",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "start": "ts-node --files src/main --transpile-only",
        "start:dev": "export NODE_ENV=develop && nest start",
        "tsc": "nodemon --watch src -e ts --exec \"(tsc && node dist/UsersService/src/main.js) || exit 1\"",
        "clean": "rm -Rf ./dist/",
        "build:ci": "npm run build",
        "build": "npm run clean && mkdir ./dist && tsc --project tsconfig.build.json",
        "prettier": "prettier --write {,*/**/}*.{ts,js,jsx,json}",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" NODE_ENV=test && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --silent=false",
        "jest": "NODE_ENV=test jest",
        "lint": "npx eslint -c .eslintrc.js --ext .ts './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern"
    },
    "dependencies": {
        "@nestjs/axios": "0.1.1",
        "@nestjs/common": "9.3.12",
        "@nestjs/config": "2.3.1",
        "@nestjs/core": "9.3.12",
        "@nestjs/devtools-integration": "0.1.3",
        "@nestjs/jwt": "10.0.2",
        "@nestjs/mapped-types": "^1.2.0",
        "@nestjs/microservices": "9.3.12",
        "@nestjs/platform-express": "9.3.12",
        "@nestjs/swagger": "6.2.1",
        "@nestjs/throttler": "3.1.0",
        "@nestjs/typeorm": "9.0.1",
        "@nestjs/websockets": "9.3.12",
        "@types/jest": "29.5.0",
        "@types/multer-s3": "3.0.0",
        "@typescript-eslint/parser": "5.56.0",
        "cache-manager": "5.2.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "cookie-parser": "1.4.6",
        "express": "^4.18.2",
        "express-prom-bundle": "6.6.0",
        "jest": "29.5.0",
        "module-alias": "2.2.2",
        "multer": "^2.0.0-rc.4",
        "mysql": "2.18.1",
        "nest-winston": "1.9.1",
        "nodemon": "2.0.22",
        "prom-client": "14.2.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "7.8.0",
        "ts-loader": "9.4.2",
        "typeorm": "0.3.15",
        "uuid": "^9.0.0",
        "winston": "3.8.2"
    },
    "devDependencies": {
        "@darraghor/eslint-plugin-nestjs-typed": "3.19.0",
        "@nestjs/testing": "9.3.12",
        "@types/cookie-parser": "1.4.3",
        "@types/express": "4.17.17",
        "@types/jest": "29.5.0",
        "@types/module-alias": "2.0.1",
        "@types/multer": "1.4.7",
        "@types/node": "18.16.0",
        "@types/nodemailer": "6.4.7",
        "@types/passport-jwt": "3.0.8",
        "@types/passport-local": "1.0.35",
        "@types/supertest": "2.0.12",
        "@types/uuid": "8.3.4",
        "@typescript-eslint/type-utils": "5.56.0",
        "jest": "29.5.0",
        "prettier": "2.8.7",
        "supertest": "6.3.3",
        "ts-jest": "29.0.5",
        "ts-node": "10.9.1",
        "typescript": "4.9.5"
    },
    "overrides": {
        "@nestjs/platform-express": {
            "multer": "^1.4.5-lts.1"
        }
    },
    "jest": {
        "preset": "ts-jest",
        "moduleFileExtensions": [
            "js",
            "json",
            "ts",
            "tsx"
        ],
        "rootDir": "test",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
